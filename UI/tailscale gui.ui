<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.1 -->
<interface>
  <!-- interface-name tailscale gui.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.4"/>
  <requires lib="webkitgtk" version="6.0"/>
  <object class="AdwApplicationWindow">
    <child>
      <object class="GtkBox">
        <property name="baseline-child">2</property>
        <property name="baseline-position">bottom</property>
        <property name="halign">center</property>
        <property name="height-request">0</property>
        <child>
          <object class="GtkBox">
            <property name="halign">baseline-center</property>
            <property name="margin-start">50</property>
            <property name="margin-top">30</property>
            <property name="orientation">vertical</property>
            <property name="valign">start</property>
            <child>
              <object class="GtkBox">
                <property name="css-name">list</property>
                <child>
                  <object class="AdwSwitchRow">
                    <property name="active">True</property>
                    <property name="height-request">50</property>
                    <property name="hexpand">True</property>
                    <property name="subtitle">Connection status</property>
                    <property name="title">User email</property>
                    <property name="width-request">300</property>
                    <style>
                      <class name="boxed-list"/>
                    </style>
                  </object>
                </child>
                <style>
                  <class name="boxed-list"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkListBox">
                <property name="margin-top">30</property>
                <child>
                  <object class="AdwSwitchRow">
                    <property name="active">True</property>
                    <property name="height-request">50</property>
                    <property name="hexpand">True</property>
                    <property name="title">SSH</property>
                    <property name="width-request">300</property>
                    <style>
                      <class name=".row"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwSwitchRow">
                    <property name="active">True</property>
                    <property name="height-request">50</property>
                    <property name="hexpand">True</property>
                    <property name="title">Advertise As Exit Node</property>
                    <property name="width-request">300</property>
                    <style>
                      <class name=".row"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="AdwSwitchRow">
                    <property name="active">True</property>
                    <property name="height-request">50</property>
                    <property name="hexpand">True</property>
                    <property name="title">Accept Routes</property>
                    <property name="width-request">300</property>
                    <style>
                      <class name=".row"/>
                    </style>
                  </object>
                </child>
                <style>
                  <class name="boxed-list"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkListBox">
                <property name="margin-top">30</property>
                <child>
                  <object class="AdwExpanderRow">
                    <property name="subtitle">Disabled</property>
                    <property name="title">Exit Nodes</property>
                    <child>
                      <object class="GtkListBox">
                        <property name="css-name">row</property>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title">exit node1</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title">exit node 2</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow">
                            <property name="title">exit node 3</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <style>
                  <class name="boxed-list"/>
                </style>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="halign">start</property>
                <property name="margin-top">30</property>
                <property name="valign">center</property>
                <child>
                  <object class="GtkButton">
                    <property name="label">Share Files</property>
                    <style>
                      <class name="suggested-action"/>
                      <class name="pill"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkButton">
                    <property name="label">Recive Files</property>
                    <style>
                      <class name="suggested-action"/>
                      <class name="pill"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="can-shrink">True</property>
                <property name="label">Share Files</property>
                <property name="margin-top">30</property>
                <property name="valign">start</property>
                <property name="visible">False</property>
                <property name="width-request">38</property>
                <style>
                  <class name="destructive-action"/>
                  <class name="pill"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="WebKitWebView">
            <property name="height-request">600</property>
            <property name="related-view">
              <object class="WebKitWebView">
                <property name="web-extension-mode">manifestv3</property>
              </object>
            </property>
            <property name="settings">
              <object class="WebKitSettings"/>
            </property>
            <property name="web-context">
              <object class="WebKitWebContext"/>
            </property>
            <property name="width-request">700</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
